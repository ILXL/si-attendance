{
    "version": 2,
    "public": "public",
    "builds": [
        {
            "src": "src/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/signin",
            "dest": "src/index.py"
        },
        {
            "src": "/getcourses",
            "dest": "src/index.py"
        },
        {
            "src": "/noncwidsignin",
            "dest": "src/index.py"
        },
        {
            "src": "/debug",
            "dest": "src/index.py"
        }
    ]
}
