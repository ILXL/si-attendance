{
    "version": 2,
    "public": ".",
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "public/index.html",
                    "public/global.css",
                    "public/favicon.png",
                    "public/build/bundle.js",
                    "public/build/bundle.css",
                    "public/build/bundle.js.map",
                    "public/build/.gitkeep"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/signin",
            "dest": "index.py"
        },
        {
            "src": "/getcourses",
            "dest": "index.py"
        },
        {
            "src": "/noncwidsignin",
            "dest": "index.py"
        },
        {
            "src": "/debug",
            "dest": "index.py"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}
