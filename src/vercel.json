{
    "version": 2,
    "public": ".",
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/signin",
            "dest": "index.py"
        },
        {
            "src": "/getcourses",
            "dest": "index.py"
        },
        {
            "src": "/noncwidsignin",
            "dest": "index.py"
        },
        {
            "src": "/debug",
            "dest": "index.py"
        },
        {
            "src": "/(.*)",
            "dest": "index.py"
        }
    ]
}
